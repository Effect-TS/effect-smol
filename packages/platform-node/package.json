{
  "name": "@effect/platform-node",
  "version": "0.77.6",
  "description": "Platform specific implementations for the Node.js runtime",
  "keywords": [
    "algebraic-data-types",
    "functional-programming",
    "node",
    "typescript"
  ],
  "homepage": "https://effect.website",
  "bugs": {
    "url": "https://github.com/Effect-TS/effect/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/platform-node"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "src/**/*.ts"
  ],
  "type": "module",
  "sideEffects": [],
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./internal/*": null,
    "./*/index": null
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./dist/index.js",
      "./*": "./dist/*.js",
      "./internal/*": null,
      "./*/index": null
    },
    "provenance": true
  },
  "scripts": {
    "codegen": "effect-utils codegen",
    "build": "tsc -b tsconfig.json && pnpm babel",
    "build:tsgo": "tsgo -b tsconfig.json && pnpm babel",
    "babel": "babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@effect/platform-node-shared": "workspace:^",
    "mime": "^4.1.0",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@testcontainers/mysql": "^11.11.0",
    "@testcontainers/postgresql": "^11.10.0",
    "@testcontainers/redis": "^11.10.0",
    "@types/node": "^25.0.6",
    "effect": "workspace:^"
  },
  "peerDependencies": {
    "effect": "workspace:^",
    "ioredis": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "tags": [
    "node",
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ]
}
