{
  "name": "@effect/openapi-generator",
  "version": "0.4.13",
  "description": "Generate Effect Schema types and HTTP clients from OpenAPI specifications",
  "keywords": [
    "codegen",
    "openapi",
    "typescript"
  ],
  "homepage": "https://effect.website",
  "bugs": {
    "url": "https://github.com/Effect-TS/effect/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/tools/openapi-generator"
  },
  "bin": {
    "openapigen": "./src/bin.ts"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "src/**/*.ts"
  ],
  "type": "module",
  "sideEffects": [],
  "exports": {
    "./package.json": "./package.json",
    "./*": "./src/*.ts",
    "./bin": null,
    "./main": null
  },
  "publishConfig": {
    "bin": {
      "openapigen": "./dist/bin.js"
    },
    "exports": {
      "./package.json": "./package.json",
      "./*": "./dist/*.js",
      "./bin": null,
      "./main": null
    },
    "provenance": true
  },
  "scripts": {
    "build": "tsc -b tsconfig.json && pnpm babel",
    "build:tsgo": "tsgo -b tsconfig.json && pnpm babel",
    "babel": "babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@types/swagger2openapi": "^7.0.4",
    "effect": "workspace:^",
    "json-schema-typed": "^8.0.2",
    "openapi-typescript": "^7.10.1",
    "swagger2openapi": "^7.0.8",
    "yaml": "^2.8.2"
  },
  "peerDependencies": {
    "@effect/platform-node": "workspace:^",
    "effect": "workspace:^"
  },
  "tags": [
    "typescript",
    "openapi",
    "codegen"
  ]
}
