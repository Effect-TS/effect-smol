{
  "name": "effect",
  "type": "module",
  "version": "4.0.0",
  "license": "MIT",
  "description": "The missing standard library for TypeScript, for writing production-grade software.",
  "homepage": "https://effect.website",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/effect"
  },
  "bugs": {
    "url": "https://github.com/Effect-TS/effect/issues"
  },
  "tags": [
    "typescript",
    "error-handling",
    "concurrency",
    "observability"
  ],
  "keywords": [
    "typescript",
    "error-handling",
    "concurrency",
    "observability"
  ],
  "sideEffects": [],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./config": "./src/config/index.ts",
    "./platform": "./src/platform/index.ts",
    "./schema": "./src/schema/index.ts",
    "./unstable/http": "./src/unstable/http/index.ts",
    "./unstable/socket": "./src/unstable/socket/index.ts",
    "./unstable/tracing": "./src/unstable/tracing/index.ts",
    "./*": "./src/*.ts",
    "./internal/*": null,
    "./*/index": null
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "provenance": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./config": {
        "default": "./dist/config/index.js",
        "types": "./dist/config/index.d.ts"
      },
      "./platform": {
        "default": "./dist/platform/index.js",
        "types": "./dist/platform/index.d.ts"
      },
      "./schema": {
        "default": "./dist/schema/index.js",
        "types": "./dist/schema/index.d.ts"
      },
      "./unstable/http": {
        "default": "./dist/unstable/http/index.js",
        "types": "./dist/unstable/http/index.d.ts"
      },
      "./unstable/socket": {
        "default": "./dist/unstable/socket/index.js",
        "types": "./dist/unstable/socket/index.d.ts"
      },
      "./unstable/tracing": {
        "default": "./dist/unstable/tracing/index.js",
        "types": "./dist/unstable/tracing/index.d.ts"
      },
      "./*": {
        "default": "./dist/*.js",
        "types": "./dist/*.d.ts"
      },
      "./internal/*": null,
      "./*/index": null
    }
  },
  "scripts": {
    "codegen": "build-utils prepare-v4",
    "build": "tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
    "test-types": "tstyche",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@types/node": "^24.0.12",
    "ajv": "^8.17.1",
    "arktype": "^2.1.20",
    "ast-types": "^0.14.2",
    "immer": "^10.1.1",
    "tinybench": "^4.0.1",
    "valibot": "^1.1.0"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "fast-check": "^4.2.0",
    "find-my-way-ts": "^0.1.6",
    "multipasta": "^0.2.5"
  }
}
